# Server
SERVER_PORT=8005
GIN_MODE=debug

# Database
DB_HOST=localhost
DB_PORT=5432
DB_USER=saas_user
DB_PASSWORD=saas_pass
DB_NAME=saas_db
DB_SCHEMA=fulfillment
DB_SSLMODE=disable

# JWT (shared with auth-service)
JWT_SECRET_KEY=your-secret-key-change-in-production

# Internal Service Communication
INTERNAL_SECRET=internal-service-secret

# AWS Lightsail
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1

# Node Configuration
NODE_BUNDLE_ID=nano_3_0
NODE_BLUEPRINT_ID=debian_12
SINGBOX_INSTALL_URL=https://raw.githubusercontent.com/antsbtw/otun-node-agent/main/install.sh

# SSH Configuration
SSH_USERNAME=admin
SSH_TIMEOUT_SECONDS=300

# Encryption (32 characters for AES-256)
ENCRYPTION_KEY=your-32-character-encryption-key

# Service Dependencies
SUBSCRIPTION_SERVICE_URL=http://localhost:8003
LICENSE_SERVICE_URL=http://localhost:8004
